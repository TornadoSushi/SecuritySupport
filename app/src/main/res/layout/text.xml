<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent">

    <com.infinity.jerry.securitysupport.common.z_utils.z_widget.ZTitleBar
        android:layout_width="match_parent"
        android:id="@+id/titleBar"
        android:layout_height="@dimen/actionBar_height">

    </com.infinity.jerry.securitysupport.common.z_utils.z_widget.ZTitleBar>

    <ScrollView
        android:layout_margin="@dimen/normal_dis"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/userInfo"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/color_main"
            android:gravity="center"
            android:text="userInfo"
            android:textColor="@color/color_white" />


        <TextView
            android:layout_marginTop="@dimen/normal_dis"
            android:id="@+id/checkItem"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/color_main"
            android:gravity="center"
            android:text="checkItem"
            android:textColor="@color/color_white" />

        <TextView
            android:id="@+id/company"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="@dimen/normal_dis"
            android:background="@color/color_main"
            android:gravity="center"
            android:text="company"
            android:textColor="@color/color_white" />

        <TextView
            android:id="@+id/baseinfo"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="@dimen/normal_dis"
            android:background="@color/color_main"
            android:gravity="center"
            android:text="base"
            android:textColor="@color/color_white" />

        <TextView
            android:id="@+id/produce"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="@dimen/normal_dis"
            android:background="@color/color_main"
            android:gravity="center"
            android:text="produce"
            android:textColor="@color/color_white" />

        <TextView
            android:id="@+id/safty"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="@dimen/normal_dis"
            android:background="@color/color_main"
            android:gravity="center"
            android:text="safty"
            android:textColor="@color/color_white" />

        <TextView
            android:id="@+id/door"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="@dimen/normal_dis"
            android:background="@color/color_main"
            android:gravity="center"
            android:text="door"
            android:textColor="@color/color_white" />

        <TextView
            android:id="@+id/shaft"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="@dimen/normal_dis"
            android:background="@color/color_main"
            android:gravity="center"
            android:text="shaft"
            android:textColor="@color/color_white" />

        <TextView
            android:id="@+id/six"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="@dimen/normal_dis"
            android:background="@color/color_main"
            android:gravity="center"
            android:text="six"
            android:textColor="@color/color_white" />

        <TextView
            android:id="@+id/checkId"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="@dimen/normal_dis"
            android:background="@color/color_main"
            android:gravity="center"
            android:text="checkId"
            android:textColor="@color/color_white" />
    </LinearLayout>

    </ScrollView>


</LinearLayout>
